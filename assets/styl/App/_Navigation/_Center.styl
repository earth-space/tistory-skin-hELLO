#__nav
  &.sticky
    .center
      display block
  .center
    line-height 1rem
    text-align center
    flex-direction column
    display none
    > *
      TextOverflowHidden()
      max-width calc(80vw - 152px)
    .title, .category
      font-weight 500
    .title
      margin 0
      font-size 1rem
      font-weight 600
      height 20px
      color var(--h-color)
      font-family "GimpoGothicBold00"
    .category
      font-size .8rem
      margin-bottom 3px
      display inline-block
